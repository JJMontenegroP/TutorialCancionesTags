[coverage:run]
relative_files = True
branch = True
source = src/
# omit = bad_file.py

[paths]
source =
    src/
    tests/
    */site-packages/

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug

    raise AssertionError
    raise NotImplementedError

    if 0:
    if __name__ == .__main__.